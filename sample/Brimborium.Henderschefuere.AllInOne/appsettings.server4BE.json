{
  "Urls": "https://localhost:5004/",
  "Comment": "The back end - inner front end",
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://localhost:5004"
      }
    }
  },
  "ReverseProxy": {
    "Tunnels": {
      "tunnelAT4": {
        "Url": "https://localhost:5001",
        "RemoteTunnelId": "betaFE1",
        "Transport": "TunnelHTTP2",
        "Authentication": {}
      },
      "tunnelBT4": {
        "Url": "https://localhost:5002",
        "RemoteTunnelId": "betaFE2",
        "Transport": "TunnelHTTP2",
        "Authentication": {}
      }
    },
    "Routes": {
      "route1T4": {
        "ClusterId": "alphaT4",
        "Match": {
          "Path": "{**catch-all}"
        }
      },
      "route2T4": {
        "ClusterId": "betaT4",
        "Match": {
          "Path": "/beta/{**catch-all}"
        }
      },
      "route3T4": {
        "ClusterId": "gammaT4",
        "Match": {
          "Path": "/example"
        }
      }
    },
    "Clusters": {
      "alphaT4": {
        "Destinations": {
          "alpha": {
            "Address": "https://localhost:5005"
          }
        }
      },
      "betaT4": {
        "Destinations": {
          "beta": {
            "Address": "https://localhost:5006"
          }
        }
      },
      "gammaT4": {
        "Destinations": {
          "gamma/destination1": {
            "Address": "https://example.com/"
          }
        }
      }
    }
  }
}
