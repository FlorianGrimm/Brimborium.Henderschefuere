{
  "Urls": "https://localhost:5003/",
  "Comment": "The back end - inner front end",
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://localhost:5003"
      }
    }
  },
  "Henderschefuere": {
    "Tunnels": {
      "tunnel1T3": {
        "Url": "https://localhost:5001",
        "RemoteTunnelId": "alphaFE1",
        "Transport": "TunnelHTTP2",
        "Authentication": {}
      },
      "tunnel2T3": {
        "Url": "https://localhost:5002",
        "RemoteTunnelId": "alphaFE2",
        "Transport": "TunnelHTTP2",
        "Authentication": {}
      }
    },
    "Routes": {
      "route1T3": {
        "ClusterId": "alphaT3",
        "Match": {
          "Path": "{**catch-all}"
        }
      },
      "route2T3": {
        "ClusterId": "betaT3",
        "Match": {
          "Path": "/beta/{**catch-all}"
        }
      },
      "route3T3": {
        "ClusterId": "gammaT3",
        "Match": {
          "Path": "/example"
        }
      }
    },
    "Clusters": {
      "alphaT3": {
        "Destinations": {
          "alpha": {
            "Address": "https://localhost:5005"
          }
        }
      },
      "betaT3": {
        "Destinations": {
          "beta": {
            "Address": "https://localhost:5006"
          }
        }
      },
      "gammaT3": {
        "Destinations": {
          "gamma/destination1": {
            "Address": "https://example.com/"
          }
        }
      }
    }
  }
}
