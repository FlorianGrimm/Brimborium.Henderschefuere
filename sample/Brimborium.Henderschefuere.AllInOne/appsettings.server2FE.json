{
  "Urls": "https://localhost:5002/",
  "Comment": "The tunnel front end",
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://localhost:5002"
      }
    }
  },
  "Henderschefuere": {
    "Routes": {
      "route1FE2": {
        "ClusterId": "alphaFE2",
        "Match": {
          "Path": "{**catch-all}",
          "Order": 100
        }
      },
      "route2FE2": {
        "ClusterId": "betaFE2",
        "Match": {
          "Path": "/beta/{**catch-all}",
          "Order": 100
        }
      },
      "route3FE2": {
        "ClusterId": "gammaFE2",
        "Match": {
          "Path": "/example"
        }
      }
    },
    "Clusters": {
      "alphaFE2": {
        "Transport": "TunnelHTTP2",
        "Authentication": {}
      },
      "betaFE2": {
        "Transport": "TunnelHTTP2",
        "Authentication": {}
      },
      "gammaFE2": {
        "Destinations": {
          "beta/destination1": {
            "Address": "https://example.com/"
          }
        },
        "CommentTransport": "HTTP"
      }
    }
  }
}
