{
  "Urls": "https://localhost:5001/",
  "Comment": "The tunnel front end",
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://localhost:5001"
      }
    }
  },
  "ReverseProxy": {
    "Routes": {
      "route1FE1": {
        "ClusterId": "alphaFE1",
        "Match": {
          "Path": "{**catch-all}"
        }
      },
      "route2FE1": {
        "ClusterId": "betaFE1",
        "Match": {
          "Path": "/beta/{**catch-all}"
        }
      },
      "route3FE1": {
        "ClusterId": "gammaFE1",
        "Match": {
          "Path": "/example"
        }
      }
    },
    "Clusters": {
      "alphaFE1": {
        "Transport": "TunnelHTTP2",
        "Authentication": {}
      },
      "betaFE1": {
        "Transport": "TunnelHTTP2",
        "Authentication": {}
      },
      "gammaFE1": {
        "Destinations": {
          "beta/destination1": {
            "Address": "https://example.com/"
          }
        },
        "Transport": "ForwardHttp"
      }
    }
  }
}
